<?xml version='1.0' encoding='utf-8'?>
<units>
  <!--base unit of distance is centimeter-->
    <unit>
      <name>Centimeter</name>
      <symbol>cm</symbol>
      <ratio>1</ratio>
    </unit>

  <!--1 meter = 100 centimeters-->
    <unit>
      <name>Meter</name>
      <symbol>m</symbol>
      <ratio>100</ratio>
    </unit>

  <!--1 meter = 0.1 centimeters-->
  <unit>
    <name>Milimeter</name>
    <symbol>mm</symbol>
    <ratio>0.1</ratio>
  </unit>
  
</units>